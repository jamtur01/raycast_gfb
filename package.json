{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "gfb",
  "title": "Get FotMob Fixtures",
  "description": "Returns football results from FotMob's API for specific leagues and teams",
  "icon": "extension_icon.png",
  "author": "jamtur01",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Show Fixtures",
      "description": "Returns football fixtures",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.5",
    "fotmob": "^2.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
